.hide-on-large-only.nav-padding
	#nav-wrapper
		.nav-trigger
			svg(viewbox="0 0 100 100", preserveAspectRatio="none")
				polygon(points="0 0, 100 0, 0 100")
			a.button-collapse(href="#", data-activates="mobile-nav")
				i.material-icons menu

.navbar-fixed.hide-on-large-only
	nav
		.nav-wrapper 
			ul.side-nav.grey.darken-3#mobile-nav
				li
					a.waves-effect.waves-dark(href="#about_me") About me
				li.divider.grey.darken-4
				li
					a.waves-effect.waves-dark(href="#websites") Websites
				li
					a.waves-effect.waves-dark(href="#games") Games
				li
					a.waves-effect.waves-dark(href="#apps") Apps & more
				li.divider.grey.darken-4
				li
					a.waves-effect.waves-dark(href="#support_me") Support me
				li
					a.waves-effect.waves-dark(href="#contact") Contact

.vertical-bullet-menu.hide-on-med-and-down.close
	ul
		li
			a#previous(href="#") 
				i.material-icons keyboard_arrow_up
		li
			a(href="#main") 
				i.fa.fa-bullseye(aria-hidden="true")
				span Home
					
		li
			a(href="#about_me") 
				i.fa.fa-bullseye(aria-hidden="true")
				span About&nbsp;me
		li
			a(href="#websites")
				i.fa.fa-bullseye(aria-hidden="true")
				span Websites
		li
			a(href="#games")
				i.fa.fa-bullseye(aria-hidden="true")
				span Games
		li
			a(href="#apps")
				i.fa.fa-bullseye(aria-hidden="true")
				span Apps&nbsp;&&nbsp;more
		li
			a(href="#support_me")
				i.fa.fa-bullseye(aria-hidden="true")
				span Support&nbsp;me
		li
			a(href="#contact") 
				i.fa.fa-bullseye(aria-hidden="true")
				span Contact
		li
			a.disabled#next(href="#") 
				i.material-icons keyboard_arrow_down